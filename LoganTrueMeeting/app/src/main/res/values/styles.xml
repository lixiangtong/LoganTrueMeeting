<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowBackground">@color/white_247x3</item>
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimary">@color/start_color</item>
        <item name="colorPrimaryDark">#ffffff</item>
        <item name="colorAccent">#cccccc</item>
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="radioButton_style" >
        <!-- Customize your theme here. -->
        <item name="android:button">@null</item>
        <item name="android:background">@null</item>
        <item name="android:drawablePadding">-15dp</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="title_bar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">64dp</item>
        <item name="android:background">@drawable/shape_home_gradient</item>
    </style>

    <style name="NetworkSettings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="et_changepassword">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:inputType">textPassword</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="tv_changepassword">
        <item name="android:layout_width">75dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:gravity">right</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#6b8699</item>
    </style>

    <style name="view_segmentation">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.05dp</item>
        <item name="android:background">@color/gray</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="TextView_About1">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">#6b8699</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingLeft">7dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="TextView_About2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#95a5b0</item>
        <item name="android:textSize">12sp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="button_login">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="EditText_Account_Style">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:visibility">visible</item>
    </style>

    <style name="TextView_Account_Style">
        <item name="android:textColor">#95a5b0</item>
        <item name="android:textSize">12sp</item>
        <item name="android:visibility">visible</item>
    </style>

    <style name="TextView_Meeting_Style">
        <item name="android:textColor">#95a5b0</item>
        <item name="android:textSize">12sp</item>
        <item name="android:visibility">visible</item>
    </style>

    <style name="EditText_Meeting_Style">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:visibility">visible</item>
    </style>

    <style name="TextView_Home_Fragment_I">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#58646d</item>
    </style>

    <style name="TextView_Home_Fragment_II">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#58646d</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="TextView_Home_Fragment_III">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#58646d</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="title_bar_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">64dp</item>
        <item name="android:background">@drawable/shape_home_gradient</item>
        <item name="android:paddingTop">30dp</item>
    </style>

    <style name="ConfirmDialog" parent="@android:style/Theme.Holo.Dialog.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="CommonBtn">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:background">@drawable/btn_common_bg</item>
    </style>
    <style name="CommonCheckBox">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/btn_common_bg</item>
    </style>

    <style name="CommonRadioButton">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:button">@null</item>
    </style>

    <style name="textView_meeting_info">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">17dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="keyboard_radioButton">
        <item name="android:layout_width">29dp</item>
        <item name="android:layout_height">29dp</item>
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/selector_keyboard</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_marginLeft">14dp</item>
    </style>

    <style name="textView_meeting_video">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/shape_textview_videoid</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:text">视频ID</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">10sp</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="video_wall_mode4">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:background">@mipmap/video_background</item>
        <item name="android:visibility">visible</item>
    </style>
    <style name="relativelayout_video_wall_mode4">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="linearlayout_video_wall_mode4">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:background">@mipmap/video_background</item>
    </style>

    <style name="video_wall_mode5">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:background">@mipmap/video_background</item>
        <item name="android:visibility">visible</item>
    </style>

    <style name="relativelayout_video_wall_mode5">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">1dp</item>
    </style>

    <style name="video_wall_mode2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:background">@mipmap/video_background</item>
        <item name="android:visibility">visible</item>
    </style>

</resources>
